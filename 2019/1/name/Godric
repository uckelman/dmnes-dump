 <span class="date">1194</span> <span class="name">Godericus</span> (<span class="case">nom</span>) <a href="../cite/Godericus/1194/RotCur-vol1"><span class="bib_key">RotCur-vol1</span> <span class="bib_loc">p. 8</span></a>;
 <span class="date">1222</span> <span class="name">Godrich</span> (<span class="case">nom</span>) <a href="../cite/Godrich/1222/StPaulDom"><span class="bib_key">StPaulDom</span> <span class="bib_loc">p. 60</span></a>,
