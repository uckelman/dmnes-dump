 <span class="date">1135</span> <span class="name">Agno</span> (<span class="case">nom</span>) <a href="../cite/Agno/1135/clairvaux-12thc"><span class="bib_key">clairvaux-12thc</span> <span class="bib_loc">7</span></a>,
 <span class="name">Agnone</span> (<span class="case">abl</span>) <a href="../cite/Agnone/1135/clairvaux-12thc"><span class="bib_key">ibid.</span> <span class="bib_loc">7</span></a>;
