<dt><span class="area">Italy</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
<dd><span class="dim">●</span> <span class="date">1265x1339</span> <span class="name">Guarini</span> (<span class="case">gen</span>) <a href="../cite/Guarini/1265x1339/BBC-Berg"><span class="bib_key">BBC-Berg</span> <span class="bib_loc">311</span></a></dd>
</dl>
</dd>
<dt><span class="area">Scotland</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
<dd><span class="dim">●</span> <span class="date">1381x1388</span> <span class="name">Warino</span> (<span class="case">abl</span>) <a href="../cite/Warino/1381x1388/CartNBer"><span class="bib_key">CartNBer</span> <span class="bib_loc">[p. xxxvii]</span></a></dd>
</dl>
</dd>
<dt><span class="area">Switzerland</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
