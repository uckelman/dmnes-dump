<script>
  var ex = document.getElementById('example');
  var classes = [
    'nym', 'gen', 'etym', 'usg', 'def', 'area', 'citelang',
    'dim', 'date', 'name', 'case', 'bib_key', 'bib_loc', 'citeme'
  ];

  function showPage(pnum) {
    for (var i = 0; i < 15; i++) {
      var e = document.getElementById(i);
      e.style.visibility = (i == pnum ? 'visible' : 'hidden');
    }

    if (pnum > 0) {
      for (var i = classes.length; i--; ) {
        var es = ex.getElementsByClassName(classes[i]);
        for (var j = es.length; j--; ) {
          es[j].style.backgroundColor = (pnum-1 == i ? '#ffaaaa' : 'transparent');
        }
      }
    }
  }

  showPage(0);

  window.onhashchange = function() {
    showPage(window.location.hash.substr(1));
  };

</script>

