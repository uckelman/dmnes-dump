</dl>
</dd>
<dt><span class="area">France</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
<dd><span class="dim">◑</span> <span class="date">1040x1080</span> <span class="name">Gonzolenus</span> (<span class="case">nom</span>) <a href="../cite/Gonzolenus/1040x1080/StVdM-II"><span class="bib_key">StVdM-II</span> <span class="bib_loc">664</span></a>;
