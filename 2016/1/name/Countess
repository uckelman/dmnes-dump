</dl>
</dd>
<dt><span class="area">Italy</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
<dd><span class="dim">●</span> <span class="date">1265x1339</span> <span class="name">Contessa</span> (<span class="case">nom</span>) <a href="../cite/Contessa/1265x1339/BBC-Berg"><span class="bib_key">BBC-Berg</span> <span class="bib_loc">17</span></a>;
 <span class="date">1310</span> <span class="name">Contissa</span> (<span class="case">nom</span>) <a href="../cite/Contissa/1310/MonRav-2"><span class="bib_key">MonRav-2</span> <span class="bib_loc">CCCL</span></a>;
