 <span class="date">1561</span> <span class="name">Esaïe</span> <a href="../cite/Esaïe/1561/RegPCC-1"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 5</span></a>;
 <span class="date">1564</span> <span class="name">Ysaye</span> <a href="../cite/Ysaye/1564/RegPCC-1"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 56</span></a>;
 <span class="date">1566</span> <span class="name">Esaye</span> <a href="../cite/Esaye/1566/RegPCC-1"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 208</span></a>;
 <span class="date">1567</span> <span class="name">Esaye</span> <a href="../cite/Esaye/1567/RegPCC-1"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 322</span></a>;
