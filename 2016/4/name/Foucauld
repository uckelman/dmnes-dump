 <span class="date">882</span> <span class="name">Fulcoldus</span> (<span class="case">nom</span>) <a href="../cite/Fulcoldus/882/InsMacon"><span class="bib_key">InsMacon</span> <span class="bib_loc">XIII</span></a>;
 <span class="date">1047</span> <span class="name">Fucaudi</span> (<span class="case">gen</span>) <a href="../cite/Fucaudi/1047/CartNDSaintes"><span class="bib_key">CartNDSaintes</span> <span class="bib_loc">I</span></a>;
 <span class="date">a1116</span> <span class="name">Fulcaldus</span> (<span class="case">nom</span>) <a href="../cite/Fulcaldus/a1116/CartStPC"><span class="bib_key">CartStPC</span> <span class="bib_loc">XX</span></a>;
