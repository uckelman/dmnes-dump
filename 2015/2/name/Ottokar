 <span class="date">1144</span> <span class="name">Otacher</span> (<span class="case">nom</span>) <a href="../cite/Otacher/1144/mairhofer"><span class="bib_key">ibid.</span> <span class="bib_loc">XV</span></a>;
 <span class="date">1145</span> <span class="name">Otacher</span> (<span class="case">nom</span>) <a href="../cite/Otacher/1145/mairhofer"><span class="bib_key">ibid.</span> <span class="bib_loc">XIX</span></a>;
