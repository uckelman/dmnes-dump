 <span class="date">c823</span> <span class="name">Teutbardus</span> (<span class="case">nom</span>) <a href="../cite/Teutbardus/c823/irminon-vol2"><span class="bib_key">irminon-vol2</span> <span class="bib_loc">27, Palatiolum</span></a>,
 <span class="name">Teutbertus</span> (<span class="case">nom</span>) <a href="../cite/Teutbertus/c823/irminon-vol2"><span class="bib_key">ibid.</span> <span class="bib_loc">19, Palatiolum</span></a>;
 <span class="date">10thC</span> <span class="name">Teotbert</span> (<span class="case">gen</span>) <a href="../cite/Teotbert/10thC/St-Bernard-Romans"><span class="bib_key">St-Bernard-Romans</span> <span class="bib_loc">no. 114</span></a>;
