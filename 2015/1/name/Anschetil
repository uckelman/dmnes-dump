<p>
  <span class="nym">Anschetil</span>
  <span class="gen">m.</span>
  
  <span class="etym"><span class="lang">Old High German</span> <i>*ans, ans-, ansi-</i> 'god, deity' + <span class="lang">Old Saxon</span> <i>ketil</i>, <span class="lang">Old Norse</span> <i>ketill</i> 'kettle'.
</span>
  
</p>
  
  


<dl>
<dt><span class="area">France</span></dt>
<dd>
<dl>
<dt><span class="lang">Latin</span></dt>
<dd><span class="dim">●</span> <span class="date">1180</span> <span class="name">Anskitilli</span> (<span class="case">gen</span>) <a href="../cite/Anskitilli/1180/RotScaNor"><span class="bib_key">RotScaNor</span> <span class="bib_loc">p. 35</span></a>,
 <span class="name">Anskitillo</span> (<span class="case">abl</span>) <a href="../cite/Anskitillo/1180/RotScaNor"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 3</span></a>,
 <span class="name">Anskitillus</span> (<span class="case">nom</span>) <a href="../cite/Anskitillus/1180/RotScaNor"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 8</span></a></dd>
<dd><span class="dim">◑</span> <span class="date">1123</span> <span class="name">Anschetini</span> (<span class="case">gen</span>) <a href="../cite/Anschetini/1123/CartNDJosaphat"><span class="bib_key">CartNDJosaphat</span> <span class="bib_loc">XII</span></a></dd>
