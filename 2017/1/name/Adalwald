 <span class="name">Æðeluuoldi</span> (<span class="case">gen</span>) <a href="../cite/Æðeluuoldi/967/CDAS"><span class="bib_key">ibid.</span> <span class="bib_loc">DXXXII</span></a>,
 <span class="name">Æðeluuoldus</span> (<span class="case">nom</span>) <a href="../cite/Æðeluuoldus/967/CDAS"><span class="bib_key">ibid.</span> <span class="bib_loc">DXXXII</span></a>,
 <span class="name">Æðelwold</span> (<span class="case">nom</span>) <a href="../cite/Æðelwold/967/CDAS"><span class="bib_key">ibid.</span> <span class="bib_loc">DXXXIII</span></a>,
