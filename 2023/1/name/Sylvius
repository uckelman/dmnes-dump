 <span class="date">11thC</span> <span class="name">Silvii</span> (<span class="case">gen</span>) <a href="../cite/Silvii/11thC/St-Barnard-Romans"><span class="bib_key">ibid.</span> <span class="bib_loc">135</span></a>,
 <span class="name">Silvius</span> (<span class="case">nom</span>) <a href="../cite/Silvius/11thC/St-Barnard-Romans"><span class="bib_key">ibid.</span> <span class="bib_loc">135</span></a>;
