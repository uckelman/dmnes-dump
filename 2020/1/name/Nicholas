 <span class="date">1582</span> <span class="name">Nicholaws</span> (<span class="case">nom</span>) <a href="../cite/Nicholaws/1582/RegCon1"><span class="bib_key">ibid.</span> <span class="bib_loc">p. 36</span></a></dd>
<dt><span class="lang">Early Modern English</span></dt>
<dd><span class="dim">●</span> <span class="date">1590</span> <span class="name">Nicholas</span> <a href="../cite/Nicholas/1590/RegLlantrithyd"><span class="bib_key">RegLlantrithyd</span> <span class="bib_loc">p. 62</span></a></dd>
<dt><span class="lang">Welsh</span></dt>
<dd><span class="dim">●</span> <span class="date">1582</span> <span class="name">Nicholas</span> <a href="../cite/Nicholas/1582/RegCon1"><span class="bib_key">RegCon1</span> <span class="bib_loc">p. 37</span></a></dd>
